{
  "name": "wajik-anime-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "dev:watch": "bun --watch src/index.ts",
    "build": "rimraf dist && tsc && tsc-alias && gulp build",
    "build:fast": "rimraf dist && bun build src/index.ts --outdir dist --target bun && gulp build",
    "build:prod": "rimraf dist && bun build src/index.ts --outdir dist --target bun --minify && gulp build",
    "start": "bun run dist/index.js",
    "start:dev": "bun src/index.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "clean": "rimraf dist node_modules/.cache"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [],
  "author": "wajik45",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.2.20",
    "@types/node": "^22.8.6",
    "gulp": "^5.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-html-minifier-terser": "^7.1.0",
    "gulp-terser": "^2.1.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "axios": "^1.7.6",
    "cheerio": "1.0.0-rc.12",
    "hono": "^4.0.0",
    "lru-cache": "^11.0.1",
    "zod": "^3.22.0"
  }
}